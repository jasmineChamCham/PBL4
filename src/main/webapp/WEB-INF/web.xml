<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  	<welcome-file>page_login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>CheckLoginServlet</servlet-name>
  	<display-name>CheckLoginServlet</display-name>
  	<description></description>
  	<servlet-class>controller.CheckLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayHostServlet</servlet-name>
  	<display-name>DisplayHostServlet</display-name>
  	<description></description>
  	<servlet-class>controller.DisplayHostServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetailHostServlet</servlet-name>
  	<display-name>DetailHostServlet</display-name>
  	<description></description>
  	<servlet-class>controller.DetailHostServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Overview_Controller</servlet-name>
  	<display-name>Overview_Controller</display-name>
  	<description></description>
  	<servlet-class>controller.Overview_Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ChangePasswordServlet</servlet-name>
  	<display-name>ChangePasswordServlet</display-name>
  	<description></description>
  	<servlet-class>controller.ChangePasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CheckLoginServlet</servlet-name>
  	<url-pattern>/CheckLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayHostServlet</servlet-name>
  	<url-pattern>/DisplayHostServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DetailHostServlet</servlet-name>
  	<url-pattern>/DetailHostServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Overview_Controller</servlet-name>
  	<url-pattern>/Overview_Controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ChangePasswordServlet</servlet-name>
  	<url-pattern>/ChangePasswordServlet</url-pattern>
  </servlet-mapping>
</web-app>